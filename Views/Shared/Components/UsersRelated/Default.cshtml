@model List<reffffound.Models.Bookmark>

<div style="margin-top:24px;"><span>you may also like this</span></div>
<div style="max-width:auto;height:auto;margin-top:24px;">

   @foreach(var bookmark in Model)
   {
	  <div style="margin:0px 20px 20px 0px;float:left;width:170px;">
		  <table>
			  <tbody>
				  <tr>
					  <td width="168" height="168" align="center">
							<a class="" asp-area="" asp-controller="Bookmarks" asp-action="Details" asp-route-guid="@bookmark.Guid" asp-route-referAction="List" asp-route-referUsername="@ViewBag.Username" asp-route-referPage="@ViewBag.CurrentPage">
								<image style="max-width:148px;max-height:148px;height:auto;" src="@bookmark.Image"></image>
							</a>
					  </td>
				  </tr>
			  </tbody>
		  </table>
		 		  
	  </div>
   }
</div>
